{
  "name": "web-pwa",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal public/logo.svg",
    "deploy": "npm run build && firebase deploy",
    "_deploy:functions": "firebase deploy --only functions",
    "deploy:db": "firebase deploy --only firestore",
    "deploy:hosting": "firebase deploy --only hosting",
    "_predeploy:hosting": "firebase target:apply hosting web-pwa budgetcalendar"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@tanstack/react-query": "^5.25.0",
    "@tanstack/react-query-devtools": "^5.25.0",
    "@tanstack/react-router": "^1.19.1",
    "@tanstack/router-devtools": "^1.19.1",
    "chakra-dayzed-datepicker": "^0.2.10",
    "chakra-react-select": "^4.7.6",
    "date-fns": "^3.3.1",
    "dayzed": "^3.2.3",
    "eslint-config-custom": "*",
    "firebase-utils": "*",
    "framer-motion": "^11.0.8",
    "i18next": "^23.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "react-spinners": "^0.13.8",
    "react-use": "^17.5.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-pwa": "^0.19.2"
  }
}
